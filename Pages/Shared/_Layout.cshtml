@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor
<!DOCTYPE html>
<html lang="en">
@{
    var fullName = HttpContextAccessor.HttpContext.Session.GetString("fullName");
}
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Language Free Admin</title>

    <!-- ==============FRAMEWORK, LIBRARY================ -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/2.10.2/umd/popper.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://unpkg.com/scrollreveal"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/jquery/latest/jquery.min.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/momentjs/latest/moment.min.js"></script>
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="~/assets/libs/sweetalert2/sweetalert2.min.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <link rel="stylesheet" href="~/assets/vendors/">
    <link rel="stylesheet" href="~/assets/vendors/typicons.font/font/typicons.css">
    <link rel="stylesheet" href="~/assets/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/style/owl.carousel.min.css">
    <link rel="stylesheet" href="~/style/tempusdominus-bootstrap-4.min.css">
    <!-- ==============STYLE================ -->
    <link rel="stylesheet" href="~/style/style_for_pages/setup.css">
    <link rel="stylesheet" href="~/style/style_for_pages/userlist.css">
    <link rel="stylesheet" href="~/style/style_for_pages/feedback.css">
    <link rel="stylesheet" href="~/style/style_for_pages/dashboard.css">
    <link rel="stylesheet" href="~/style/style_for_pages/allchart.css">
    <link rel="stylesheet" href="~/style/style_for_pages/style.css">
    <link rel="shortcut icon" href="~/assets/images/logo/logo.png" type="image/x-icon">
    <!-- ==============DATATABLE================ -->
    <link href="~/js/javascript/datatables.net-bs4/css/dataTables.bootstrap4.css" rel="stylesheet">

  @RenderSection("Scripts", required: false)
</head>

<body>
    <div class="container-scroller">
        <nav class="navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
            <div class="text-center navbar-brand-wrapper contain_logo">
                <a class="navbar-brand brand-logo" asp-page="/Index">
                    <img class="logo_header" src="~/assets/images/logo/logo_cochu.png" alt="logo" />
                </a>
                <a class="logo-new" asp-page="/Index" style="display: none;">
                    <img class="logo_small_header" src="~/assets/images/logo/logo.png" alt="logo" />
                </a>
                <button class="navbar-toggler navbar-toggler align-self-center d-none d-lg-flex" type="button"
                        data-toggle="minimize" title="Toggle Navigation">
                    <i class="typcn typcn-th-menu"></i>
                </button>
            </div>
                  
            <div class="navbar-menu-wrapper d-flex align-items-center justify-content-end">
         
                <ul class="navbar-nav navbar-nav-right">
                    
                    <li class="nav-item nav-profile dropdown">
                        <a class="nav-link dropdown-toggle  pl-0 pr-0" asp-page="/#" data-toggle="dropdown"
                           id="profileDropdown">
                            <i class="typcn typcn-user-outline mr-0"></i>
                            <span class="nav-profile-name"><b>@fullName</b></span>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right navbar-dropdown"
                             aria-labelledby="profileDropdown">
                              <a class="dropdown-item" asp-page="/Admin/EditProfileAdmin">
                                <i class="typcn typcn-pen text-primary"></i>
                                Edit Profile
                            </a>
                            <a class="dropdown-item" asp-page="/Admin/ChangePassword">
                                <i class="typcn typcn-key-outline text-primary"></i>
                                Change Password
                            </a>
                            <a class="dropdown-item" asp-page="/Login/Login">
                                <i class="typcn typcn-power text-primary"></i>
                                Logout
                            </a>
                        </div>
                    </li>
                </ul>
              
            </div>
        </nav>
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:~/~/partials/_settings-panel.html -->  
            @RenderBody()
        </div>
         
        <!-- page-body-wrapper ends -->
    </div>
    <!-- ==============FRAMEWORK, LIBRARY================ -->
    <script src="~/js/javascript/hoverable-collapse.js"></script>
    <script src="~/js/javascript/template.js"></script>
    <script src="~/js/javascript/settings.js"></script>
    <script src="~/js/javascript/todolist.js"></script>
    <script src="~/js/javascript/chart.js"></script>
    <script src="~/js/javascript/off-canvas.js"></script>
    <script src="~/js/javascript/hoverable-collapse.js"></script>
    <script src="~/js/javascript/moment-timezone.min.js"></script>
    <script src="~/js/javascript/moment.min.js"></script>
    <script src="~/js/javascript/owl.carousel.min.js"></script>
    <script src="~/js/javascript/tempusdominus-bootstrap-4.min.js"></script>
    <script src="~/assets/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/assets/vendors/progressbar.js/progressbar.min.js"></script>
    <script src="~/assets/vendors/chart.js/Chart.min.js"></script>
    <script src="~/assets/vendors//template.js"></script>
    <script src="~/assets/vendors//settings.js"></script>
    <script src="~/assets/vendors//todolist.js"></script>
    <!-- ==============SCRIPT================ -->
    <script src="~/js/javascript/javascript_for_pages/dashboard.js"></script>
    <script src="~/js/javascript/javascript_for_pages/userlist.js"></script>
    <script src="~/js/javascript/javascript_for_pages/feedback.js"></script>
    <script src="~/js/javascript/javascript_for_pages/allchart.js"></script>
    <!-- ==============DATATABLE================ -->
    <script src="~/js/javascript/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/js/javascript/datatables.net/js/jquery.dataTables.js"></script>
    <script src="~/js/datatable/datatable-basic.init.js"></script>
    <script src="~/js/javascript/javascript_for_pages/allchart.js"></script>
    <script src="~/js/javascript/javascript_for_pages/searchall.js"></script>
     <!-- ==============DatePciker================ -->
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js"></script>

</body>

</html>